  "require": {
    "php": "~8.4.0 || ~8.5.0",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "composer-runtime-api": "^2.0",
    "browscap/browscap-php": "^7.5.0",
    "cbschuld/browser.php": "^2.0.0",
    "doctrine/dbal": "^4.3.3",
    "donatj/phpuseragentparser": "^1.10.0",
    "endorphin-studio/browser-detector": "^6.0.1",
    "endorphin-studio/browser-detector-data": "^1.2.0",
    "endorphin-studio/browser-detector-tests": "^2.2.0 || ^3.1.0",
    "guzzlehttp/guzzle": "^7.10.0",
    "guzzlehttp/psr7": "^2.8.0",
    "jaybizzle/crawler-detect": "^1.3.5",
    "laminas/laminas-diactoros": "^3.6.0",
    "league/flysystem": "^3.30.0",
    "matomo/device-detector": "^6.4.7",
    "matthiasmullie/scrapbook": "^1.5.4",
    "mimmi20/browscap-helper-source": "^6.0.0",
    "mimmi20/browser-detector": "^10.0.13",
    "mimmi20/ua-result": "^12.0.0",
    "mobiledetect/mobiledetectlib": "^4.8.09",
    "psr/cache": "^3.0.0",
    "ramsey/uuid": "^4.9.1",
    "symfony/http-foundation": "^7.3.3",
    "symfony/yaml": "^7.3.3",
    "ua-parser/uap-php": "^3.10.0",
    "whichbrowser/parser": "^2.1.8",
    "wolfcast/browser-detection": "^2.9.9",
    "woothee/woothee": "^1.10.0",
    "woothee/woothee-testset": "^1.13.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.31.2",
    "mimmi20/coding-standard": "^6.1.13",
    "nikic/php-parser": "^5.6.1",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.22",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "rector/rector": "^2.1.6",
    "rector/type-perfect": "^2.1.0",
    "shipmonk/composer-dependency-analyser": "^1.8.3",
    "symfony/process": "^7.3.3",
    "symplify/phpstan-rules": "^14.7.0",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "UserAgentParserComparison\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": {
      "browscap/browscap": "source",
      "cbschuld/browser.php": "source",
      "jaybizzle/crawler-detect": "source",
      "matomo/device-detector": "source",
      "mimmi20/browser-detector": "source",
      "mobiledetect/mobiledetectlib": "source",
      "whichbrowser/parser": "source",
      "woothee/woothee-testset": "source",
      "*": "dist"
    },
    "sort-packages": true
  }
}
