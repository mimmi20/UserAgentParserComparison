{
  "name": "mimmi20/user-agent-parser-comparison",
  "description": "Generate a comparison matrix for different UserAgent parsers",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "ThaDafinser",
      "email": "martin.keckeis1@gmail.com"
    },
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "UserAgentParserComparison\\": "src"
    }
  },
  "require": {
    "php": "^8.1.0",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "composer-runtime-api": "^2.0",
    "browscap/browscap": "^6.1.3",
    "browscap/browscap-php": "^7.2.0",
    "cbschuld/browser.php": "^v1.9.6",
    "donatj/phpuseragentparser": "^v1.7.0",
    "endorphin-studio/browser-detector": "^6.0.1",
    "endorphin-studio/browser-detector-tests": "^2.2.0 || ^3.1.0",
    "jaybizzle/crawler-detect": "^v1.2.112",
    "league/flysystem": "^3.12.2",
    "matomo/device-detector": "^6.0.6",
    "matthiasmullie/scrapbook": "^1.5.0",
    "mimmi20/browscap-helper-source": "^3.0.18",
    "mimmi20/browser-detector": "^7.0.2",
    "mobiledetect/mobiledetectlib": "^3.74.0",
    "ramsey/uuid": "^4.7.3",
    "ramsey/uuid-doctrine": "^2.0.0",
    "symfony/yaml": "^v6.2.5",
    "ua-parser/uap-php": "^v3.9.14",
    "whichbrowser/parser": "^v2.1.7",
    "wolfcast/browser-detection": "^2.9.7",
    "woothee/woothee": "^v1.10.0",
    "woothee/woothee-testset": "^v1.13.0",
    "zsxsoft/php-useragent": "^1.5.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "mimmi20/coding-standard": "4.0.0-beta.6",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.10.2",
    "phpstan/phpstan-deprecation-rules": "^1.1.2"
  },
  "config": {
    "preferred-install": {
      "browscap/browscap": "source",
      "cbschuld/browser.php": "source",
      "jaybizzle/crawler-detect": "source",
      "mimmi20/browser-detector": "source",
      "matomo/device-detector": "source",
      "mobiledetect/mobiledetectlib": "source",
      "sinergi/browser-detector": "source",
      "whichbrowser/parser": "source",
      "woothee/woothee-testset": "source",
      "*": "dist"
    },
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
